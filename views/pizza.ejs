<% include partials/header %>

<h1>Top 3 Results Near You</h1>

<div class="container result-text">
    <div class="row">
        <% var sum = 0; %>
        <% data["businesses"].forEach(function(location){ %>
            <div class="inlinePics result">
            <%= location["name"] %><br>
            <img src=<%= location["image_url"] %>><br>
            <%= location["location"]["cross_streets"] %><br>
            </div>
        <% }); %>
    </div>
</div>    
    
<a href="/">Search again</a>

<% include partials/footer %>